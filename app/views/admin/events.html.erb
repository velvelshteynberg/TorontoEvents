<h1> Events Pending Approval </h1>

<% @events.each do |event| %>
    <ul>
        <li> <%= event.name %> </li>
        <li> <%= event.address %> </li>
        <li> <%= event.description %> </li>
        <li> <%= event.start_date %> </li>
        <li> <%= event.end_date %> </li>
        <li> <%= event.start_time %> </li>
        <li> <%= event.end_time %> </li>
        <li> <%= event.host_organization_id %> </li>
        <li> <%= event.end_time %> </li>
    </ul>
<% end %>

