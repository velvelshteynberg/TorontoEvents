
                             <h1>EVENTS!!</h1>
<% flash.each do |type, msg| %>
   <div>
     <%= msg %>
   </div>
 <% end %>

 <% @events.each do |event| %>

   <p> <%= event.name %> </p>
   <p> <%= event.start_date %> - <%= event.end_date %></p>
   <p> <%= event.start_time %> - <%= event.end_time %></p>
   <p> <%= event.address %></p>
   <p> <%= event.description %></p>
   

<% end %>

