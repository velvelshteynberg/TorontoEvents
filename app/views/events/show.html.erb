<h1> Event </h1>
<%= link_to "Back to Events", events_url %> | <%= link_to "Edit", edit_event_path(@event) %> | <%= link_to "Delete", @event, method: :delete, data: {confirm: "Are you sure you want to delete this event?"} %>

<p> <%= @event.name %> </p>
<p> <%= @event.start_date %> - <%= @event.end_date %></p>
<p> <%= @event.start_time.strftime("%I:%M %p") %> - <%= @event.end_time.strftime("%I:%M %p") %></p>
<p> <%= @event.address %></p>
<p> <%= @event.description %></p>